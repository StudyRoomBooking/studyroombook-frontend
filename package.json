{
  "name": "studyroombook-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p 4000",
    "test": "npx cypress open",
    "lint": "next lint",
    "predev:instrument-clean": "rm -rf .nyc_output coverage .next",
    "dev:instrumented": "INSTRUMENT_CODE=1 next dev",
    "build:instrumented": "INSTRUMENT_CODE=1 next build",
    "cypress-run-e2e": "npx cypress run --e2e --spec \"cypress/e2e/**/*.cy.ts,!cypress/e2e/examples/**\" --headless",
    "cypress-run-component": "npx cypress run --component --headless",
    "coverage-report": "npx nyc report --reporter=html"
  },
  "dependencies": {
    "@yudiel/react-qr-scanner": "^2.0.0-beta.3",
    "antd": "^5.15.4",
    "axios": "^1.6.8",
    "date-fns": "^3.6.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "next": "^14.2.3",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^2.12.7",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.39",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "babel-plugin-istanbul": "^6.1.1",
    "cypress": "^13.10.0",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "nyc": "^15.1.0",
    "postcss": "^8",
    "source-map-support": "^0.5.21",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
